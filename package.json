{
  "name": "Adi-Zeroex",
  "version": "7.40.1",
  "description": "A messenger chat bot made by Aditta Datta Joyanto.",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "lint": "./node_modules/.bin/eslint **.js",
    "prettier": "prettier utils.js src/* --write"
  },
  "author": "Aditta Datta Joyanto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aditta-Datta/Zeroex-Project/issuse"
  },
  "homepage": "https://github.com/Aditta-Datta/Zeroex-Project#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/Aditta-Datta/Zeroex-Project.git"
  },
  "dependencies": {
    "aes-js": "latest",
    "assert": "latest",
    "bluebird": "latest",
    "chalk": "4.1.2",
    "cheerio": "latest",
    "crypto": "latest",
    "crypto-js": "latest",
    "got": "^11.8.3",
    "https-proxy-agent": "latest",
    "is-hexcolor": "^1.0.0",
    "lodash": "",
    "mqtt": "latest",
    "npmlog": "latest",
    "os": "latest",
    "path": "latest",
    "pretty-ms": "7.0.1",
    "request": "latest",
    "websocket-stream": "latest",
    "uuid-apikey": "latest",
    "express": "latest",
    "readline": "latest",
    "totp-generator": "latest",
  },
  "engines": {
    "node": ">=14.x"
  },
  "devDependencies": {
    "eslint": "latest",
    "mocha": "latest",
    "prettier": "latest"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        1,
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "keywords": [
    "Fca Zeroex",
    "Zeroex-Project",
    "Adi fca",
    "Adi"
  ]
}
